  "type": "module",
  "engines": {
    "node": "22.14.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node ./build/src/app.js",
    "start:prod": "pm2 start ./build/src/app.js --no-daemon",
    "dev": "node --env-file=.env --loader ts-node/esm ./src/app.ts",
    "builddc": "docker compose build --no-cache --pull",
    "startdc": "docker compose up -d",
    "startdb": "docker compose up -d mysql redis mongo",
    "startmysql": "docker compose up -d mysql",
    "format": "prettier --write .",
    "check": "npx eslint",
    "test:dev": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
