    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^9.12.0",
    "install": "^0.13.0",
    "npm": "^11.3.0",
    "prettier": "^3.3.3",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.7.9",
